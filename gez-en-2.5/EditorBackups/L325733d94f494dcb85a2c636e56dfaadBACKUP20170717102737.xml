            <entry xml:id="L325733d94f494dcb85a2c636e56dfaad" n="3745">
               <form>
                  <foreign xml:lang="gez">ተባሀለ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann"> III,3 (crebro <foreign xml:lang="gez">ተበሀለ፡</foreign> scriptum) <sense n="1">
                     <cit type="translation" xml:lang="la">
                        <quote>sibi invicem</quote>
                     </cit> (alios aliis) <cit type="translation" xml:lang="la">
                        <quote>dicere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>colloqui</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>conferre sermones</quote>
                     </cit>
                     <ref cRef="Gen." loc="11,4">Gen. 11,4</ref>; <ref cRef="2 Esr." loc="14,3">2
                        Esr. 14,3</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በበይናቲሆሙ፡ </foreign>
                     <ref cRef="Gen." loc="42,21">Gen. 42,21</ref>; <ref cRef="Num." loc="14,4">Num.
                        14,4</ref>; <ref cRef="Jud." loc="6,29">Jud. 6,29</ref>; <ref cRef="Jud." loc="10,18">Jud. 10,18</ref>; <ref cRef="1 Reg." loc="10,11">1 Reg.
                        10,11</ref>; <ref cRef="1 Esr." loc="3,4">1 Esr. 3,4</ref>; <ref cRef="Jon." loc="1,7">Jon. 1,7</ref>; <ref cRef="Judith" loc="7,4">Judith 7,4</ref>;
                        <ref cRef="Judith" loc="10,19">Judith 10,19</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ምስለ፡</foreign>, ut: <foreign xml:lang="gez">ተባሁሉ፡ ፩
                        ምስለ፡ ካልኡ፡</foreign>
                     <ref cRef="Gen." loc="11,3">Gen. 11,3</ref>; <ref cRef="Gen." loc="37,19">Gen.
                        37,19</ref>; <lbl expand="cum">c.</lbl> Acc: <foreign xml:lang="gez">እለ፡ይትባሀልዎ፡ ለፈርዖን፡</foreign>
                     <foreign xml:lang="grc">οἱ διαλεγόμενοι πρός</foreign>
                     <ref cRef="Ex." loc="6,27">Ex. 6,27</ref>. <sense n="a">
                        <cit type="translation" xml:lang="la">
                           <quote>colloqui et convenire </quote>
                        </cit>de re: <foreign xml:lang="gez">ተባሀሉ፡ በበይናቲሆሙ፡ ከመ፡ የሀቦ፡</foreign>
                        <ref cRef="Kuf." loc="104.">Kuf. p. 104.</ref>
                     </sense>
                     <sense n="b">
                        <cit type="translation" xml:lang="la">
                           <quote>conclamare</quote>
                        </cit> et <cit type="translation" xml:lang="la">
                           <quote>acclamare</quote>
                        </cit>: <foreign xml:lang="gez">ወተባሀሉ፡ ኵሉ፡ ሕዝብ፡ </foreign>
                        <foreign xml:lang="grc">ἐφώνησε</foreign>
                        <ref cRef="1 Esr." loc="4,41">1 Esr. 4,41</ref>.</sense>
                     <sense n="2">
                        <cit type="translation" xml:lang="la">
                           <quote>contendere</quote>
                        </cit> verbis, <cit type="translation" xml:lang="la">
                           <quote>disceptare</quote>
                        </cit>,<cit type="translation" xml:lang="la">
                           <quote> disputare</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>contradicere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>obloqui</quote>
                        </cit>: <ref cRef="3 Reg." loc="3,22">3 Reg. 3,22</ref>; <foreign xml:lang="gez">ተባሀሉ፡ በበይናቲሆሙ፡</foreign>
                        <ref cRef="Marc." loc="4,41">Marc. 4,41</ref>; <ref cRef="Marc." loc="9,34">Marc. 9,34</ref>; <foreign xml:lang="gez">ኢትትባሀል፡ ወኢትንብብ፡ እኩየ፡ ላዕለ፡ ርእሰ፡
                           ሕዝብከ፡</foreign> F.M. 20,1; <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp>
                        <lbl expand="persona">pers.</lbl> (<ref cRef="Ex." loc="6,27">Ex.
                        6,27</ref>;) <ref cRef="Judae" loc="9 ">Judae 9 </ref>(et <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">በእንተ፡</foreign> rei); <foreign xml:lang="gez">ተባህሎትክሙ፡</foreign>
                        <foreign xml:lang="grc">αἱ ἀντιλογίαι ὑμῶν</foreign>
                        <ref cRef="Deut." loc="1,12">Deut. 1,12</ref>. <cb n="0485" xml:id="c0485"/>
                     </sense>
                  </sense>
               </sense>
               
            </entry>
